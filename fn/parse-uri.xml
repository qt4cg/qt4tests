        "path-segments": [ "", "specifications", "xpath-functions-40", "Overview.html" ],
        "scheme": "https",
        "hierarchical": true(),
        "uri": "https://qt4cg.org/specifications/xpath-functions-40/Overview.html#parse-uri"
      }</assert-deep-eq>
    </result>
  </test-case>

