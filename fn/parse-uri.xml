    <test>fn:parse-uri("tel:+1-816-555-1212")</test>
    <result>
      <any-of>
        <assert-deep-eq>map {
           "uri": "tel:+1-816-555-1212",
           "scheme": "tel",
           "hierarchical": false(),
           "path": "+1-816-555-1212",
           "path-segments": array { "+1-816-555-1212" }
        }</assert-deep-eq>
        <assert-deep-eq>map {
           "uri": "tel:+1-816-555-1212",
           "scheme": "tel",
           "hierarchical": false(),
           "path": "+1-816-555-1212",
           "path-segments": array { " 1-816-555-1212" }
        }</assert-deep-eq>
      </any-of>
    </result>
  </test-case>

