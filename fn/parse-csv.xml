        <assert>deep-equal($result => map:remove("get"), 
          map{"columns":(), 
          "column-index": map{}, 
          "rows":(["one",`[{char(10)}]`],["","four"])})</assert>
        <assert>?get(1,1) = "one"</assert>
        <assert>?get(1,2) = `[{char(10)}]`</assert>
        <assert>?get(2,1) = ""</assert>
        <assert>?get(2,2) = "four"</assert>
      </all-of>
    </result>
  </test-case>
  
